    "@aptx/frontend-tk-binding-linux-arm-musleabihf": "0.1.1",
    "@aptx/frontend-tk-binding-darwin-x64": "0.1.1",
    "@aptx/frontend-tk-binding-win32-x64-msvc": "0.1.1",
    "@aptx/frontend-tk-binding-linux-x64-gnu": "0.1.1",
    "@aptx/frontend-tk-binding-linux-x64-musl": "0.1.1"
